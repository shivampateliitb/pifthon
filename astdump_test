Module(
	body=[
		Assign(
			targets=[Name(id='a', ctx=Store())], value=Str(s='hello')
		), 
		Assign(
			targets=[Name(id='b', ctx=Store())], value=Str(s='world')
		), 
		Assign(
			targets=[Name(id='c', ctx=Store())], value=BinOp(left=Name(id='a', ctx=Load()), op=Add(), right=Name(id='b', ctx=Load()))
		)
	]
)

a=0
if a<1:
    b=2
else:
    b=3

Module(
	body=[
		Assign(
			targets=[Name(id='a', ctx=Store())], value=Num(n=0)
		), 
		If(
			test=Compare(left=Name(id='a', ctx=Load()), ops=[Lt()], comparators=[Num(n=1)]), 
				body=[
					Assign(targets=[Name(id='b', ctx=Store())], value=Num(n=2))
					], 
					orelse=[
					Assign(targets=[Name(id='b', ctx=Store())], value=Num(n=3))
					]
		)
	]
)
